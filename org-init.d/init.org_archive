  ;;   (setq anything-grep-candidates-fast-directory-regexp "^/tmp")
  ;;   (setq anything-c-filelist-file-name "/tmp/all.filelist"))
  ;; ã©ã†ã‚‚ grep ä¸Šæ‰‹ãã„ã‹ãªã„ï¼all.filelist ãŒå¤§ãã™ãã‹ï¼Ÿ
  ;; ã‚ˆã—ï¼Œlinux ãªã‚‰ locate ä½¿ãˆã‚‹ã®ã§ anything-for-files ã§ã„ã„ã‹ï¼current-dir+ ã‚‚å€™è£œã«å…¥ã‚‹ã—
  (define-key mode-specific-map (kbd "r") 'anything-for-files) ; recentfile çš„ã«ä½¿ã„ãŸã„

  ;; anything-show-kill-ring
  (global-set-key (kbd "M-y") 'anything-show-kill-ring)
